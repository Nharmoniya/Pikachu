\n    content: '';\r\n    display: block;\r\n    position: absolute;\r\n    height: 28px;\r\n    width: 28px;\r\n    border:3px solid black;\r\n    border-radius: 50%;\r\n    background: white;\r\n    left: 5px;\r\n}\r\n.eye-right::before{\r\n    content: '';\r\n    display: block;\r\n    position: absolute;\r\n    height: 28px;\r\n    width: 28px;\r\n    border:3px solid black;\r\n    border-radius: 50%;\r\n    background: white;\r\n    left: 5px;\r\n}\r\n.Blush-left{\r\n   border: 4px solid black;\r\n   position: absolute;\r\n   height: 100px;\r\n   width: 100px;\r\n   background: rgb(255, 0, 0);\r\n   border-radius: 50%;\r\n   left: 50%;\r\n   right: 50%;\r\n   transform: translateX(-260px);\r\n   top: 90px;\r\n}\r\n.Blush-right{\r\n    border: 4px solid black;\r\n    position: absolute;\r\n    height: 100px;\r\n    width: 100px;\r\n    background: rgb(255, 0, 0);\r\n    border-radius: 50%;\r\n    left: 50%;\r\n    right: 50%;\r\n    transform: translateX(170px);\r\n    top: 90px;\r\n }\r\n .img{\r\n     position: absolute;\r\n     top: 50%;\r\n     left: 50%;\r\n }\r\n .Blush-left > img{\r\n     position: absolute;\r\n     transform: rotateY(180deg) translateX(100px) translateY(50px);\r\n\r\n }\r\n .Blush-right > img{\r\n     position: absolute;\r\n     transform: translateX(20px) translateY(50px);\r\n }\r\n .mouth{\r\n\r\n    width: 200px;\r\n    height: 200px;\r\n    position: absolute;\r\n    left: 50%;\r\n    top: 30px;\r\n    margin-left: -94px;\r\n }\r\n .mouth .up{\r\n     z-index: 1;\r\n\r\n }\r\n .mouth .up .left-lip{\r\n     position: relative;\r\n     border: 4px solid black;\r\n     height: 32px;\r\n     width: 100px;\r\n     border-radius: 0 0 0 50px;\r\n     border-top-color:transparent;\r\n     border-right-color: transparent;\r\n     transform: rotate(-15deg) translateX(2px);\r\n     z-index: 2;\r\n     background: rgb(255,229,0);\r\n }\r\n .mouth .up .left-lip::before{\r\n\r\n     content: '';\r\n     display: block;\r\n     width: 7px;\r\n     